generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mood {
  id          Int      @id @default(autoincrement())
  mood        String
  energy      Int
  text        String?
  lat         Float
  lng         Float
  deleteToken String?  @unique
  createdAt   DateTime @default(now())

  @@index([createdAt])
}
