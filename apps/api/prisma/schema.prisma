generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mood {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  lat         Float
  lng         Float
  mood        String
  energy      Int
  city        String?
  country     String?
  message     String?
  deleteToken  String?  @unique   // TEMP: optional so we can add & backfill
}
